{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@security/(.*)": "<rootDir>/../libs/security/src/$1",
    "@security": "<rootDir>/../libs/security/src",
    "@db/(.*)": "<rootDir>/../libs/db/src/$1",
    "@db": "<rootDir>/../libs/db/src"
  }
}
