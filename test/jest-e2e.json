{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/apple": "<rootDir>/../libs/apple/src",
    "@app/apple/(.*)": "<rootDir>/../libs/apple/src/$1",
    "@app/database": "<rootDir>/../libs/database/src",
    "@app/database/(.*)": "<rootDir>/../libs/database/src/$1",
    "@app/google": "<rootDir>/../libs/google/src",
    "@app/google/(.*)": "<rootDir>/../libs/google/src/$1",
    "@app/security": "<rootDir>/../libs/security/src",
    "@app/security/(.*)": "<rootDir>/../libs/security/src/$1"
  }
}