{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "apple": {
      "type": "library",
      "root": "libs/apple",
      "entryFile": "index",
      "sourceRoot": "libs/apple/src",
      "compilerOptions": {
        "tsConfigPath": "libs/apple/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "db": {
      "type": "library",
      "root": "libs/db",
      "entryFile": "index",
      "sourceRoot": "libs/db/src",
      "compilerOptions": {
        "tsConfigPath": "libs/db/tsconfig.lib.json"
      }
    },
    "google": {
      "type": "library",
      "root": "libs/google",
      "entryFile": "index",
      "sourceRoot": "libs/google/src",
      "compilerOptions": {
        "tsConfigPath": "libs/google/tsconfig.lib.json"
      }
    },
    "security": {
      "type": "library",
      "root": "libs/security",
      "entryFile": "index",
      "sourceRoot": "libs/security/src",
      "compilerOptions": {
        "tsConfigPath": "libs/security/tsconfig.lib.json"
      }
    }
  }
}